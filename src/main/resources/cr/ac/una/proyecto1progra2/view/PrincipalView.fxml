   <center>
      <VBox prefHeight="500.0" prefWidth="700.0" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnEditarUsuario" mnemonicParsing="false" onAction="#onAcrionBtnEditarUsuario" text="Editar Usuario" />
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
            <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#onActionBtnSalir" text="Salir" />
         </children>
      </VBox>
   </left>
</BorderPane>
